[font]
size = 12.0

[font.bold]
family = "AurulentSansM Nerd Font Mono"
style = "Bold"

[font.bold_italic]
family = "AurulentSansM Nerd Font Mono"
style = "Bold Italic"

[font.italic]
family = "AurulentSansM Nerd Font Mono"
style = "Italic"

[font.normal]
family = "AurulentSansM Nerd Font Mono"
style = "Regular"

[general]
import = ["~/.config/alacritty/themes/themes/tokyo_night_enhanced.toml"]

[terminal]
shell = { program = "/usr/bin/tmux", args = ["new", "-As0"] }
